    "import math\n",
    "\n",
    "def norm(lst):\n",
    "    sum_of_squares = sum(x ** 2 for x in lst)\n",
    "    l2_norm = math.sqrt(sum_of_squares)\n",
    "    return l2_norm\n",
    "\n",
    "# Test the function\n",
    "result = norm([1, 2, 2])\n",
    "print(result)  # Output: 3.0\n"
   ]
