    "def dict_avg(input_dict):\n",
    "    values = list(input_dict.values())\n",
    "    total_sum = sum(values)\n",
    "    avg = total_sum / len(values)\n",
    "    return avg\n",
    "\n",
    "# Test the function\n",
    "result = dict_avg({'a': 1, 'b': 2, 'c': 8, 'd': 1})\n",
    "print(result)  # Output: 3.0\n"
   ]
