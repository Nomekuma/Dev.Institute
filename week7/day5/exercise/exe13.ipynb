    "def sum_over_k(sentence, k):\n",
    "    words = sentence.split()\n",
    "    count = sum(1 for word in words if len(word) > k)\n",
    "    return count\n",
    "\n",
    "# Test the function\n",
    "sentence = 'Do or do not there is no try'\n",
    "k = 2\n",
    "result = sum_over_k(sentence, k)\n",
    "print(result)  # Output: 3\n"
   ]
  }
