    "def is_palindrome(input_string):\n",
    "    reversed_string = input_string[::-1]\n",
    "    return input_string == reversed_string\n",
    "\n",
    "# Test the function\n",
    "print(is_palindrome('radar'))  # Output: True\n",
    "print(is_palindrome('John'))   # Output: False\n"
   ]
