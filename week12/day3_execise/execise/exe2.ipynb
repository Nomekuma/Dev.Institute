   "source": [
    "import numpy as np\n",
    "\n",
    "a = np.array([1, 2, 3, np.nan, 5, 6, 7, np.nan])\n",
    "\n",
    "# Create a boolean mask to identify NaN values\n",
    "nan_mask = np.isnan(a)\n",
    "\n",
    "# Use the boolean mask to filter the array and drop NaN values\n",
    "a_without_nan = a[~nan_mask]\n",
    "\n",
    "print(a_without_nan)\n"
   ]
  }
